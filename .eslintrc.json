{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": "off",
        "max-len": ["error", { "code": 200 }],
        "camelcase" : "off",
        "no-underscore-dangle" : "off"
    }
}
